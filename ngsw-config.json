{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/favicon-32.png",
          "/apple-touch-icon.png",
          "/icon-192.png",
          "/icon-512.png",
          "/logo.jpg",
          "/manifest.webmanifest",
          "/runtime-config.js",
          "/index.html",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/**/*.(png|jpg|jpeg|svg|webp|gif|ico)",
          "/**/*.(woff|woff2|ttf|otf)"
        ]
      }
    }
  ]
}
